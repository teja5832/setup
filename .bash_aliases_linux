alias cp='cp -iv'                           # Preferred 'cp' implementation
alias mv='mv -iv'                           # Preferred 'mv' implementation
alias mkdir='mkdir -pv'                     # Preferred 'mkdir' implementation

alias c='clear'
alias h='history'

#LS
alias ls='ls -bF --color'
alias ll='ls -FGlAhp'
alias l='ls -hla'
alias la='ls -A'
alias lc='ls -CF'
alias lf='ls -F -l -G %l | grep ^-'
alias lk='ls -F -l -G %l | grep ^l'
alias lx='ls -F -l -G %l | grep ^-..x'

#git
alias st='git status'
alias gps='git push'
alias gpl='git pull'

#grep
alias grep='grep --color=auto'

alias type='whence -f'
alias type='type -f'

alias open='xdg-open'
alias diff='colordiff'

#Python
alias p='ipython3'
alias pip='pip3'
alias py="python3 -c 'print "

alias pbcopy='xclip -selection clipboard'
alias pbpaste='xclip -selection clipboard -o'
alias e='emacs -nw'
alias png='ping -c 5 www.google.com'

alias ws='ssh teja58432@rama.cs.wisc.edu'

alias d='cd ~/Desktop'
alias dw='cd ~/Downloads'
